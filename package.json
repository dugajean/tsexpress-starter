{
  "name": "typescript-express",
  "description": "Express with TypeScript and more.",
  "version": "0.1.0",
  "author": "Dukagjin Surdulli <me@dukagj.in> (https://dukagj.in/)",
  "homepage": "https://github.com/dugajean/typescript-express",
  "license": "MIT",
  "main": "server.js",
  "bugs": {
    "url": "https://github.com/dugajean/typescript-express/issues",
    "email": "me@dukagj.in"
  },
  "contributors": [
    "Dukagjin Surdulli <me@dukagj.in> (https://dukagj.in/)"
  ],
  "scripts": {
    "dev": "nodemon -e ts -w ./src -x ts-node src/server.ts",
    "build": "rimraf dist && yarn lint && tsc",
    "start": "cross-env NODE_ENV=production yarn build && node dist/server.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=test tsc && ava"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^12.7.1",
    "ava": "latest",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1",
    "standard": "latest",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "ava": {
    "failFast": true,
    "verbose": true,
    "helpers": [
      "test/helpers/**/*"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "engines": {
    "node": ">=8.3"
  },
  "keywords": [
    "typescript",
    "express",
    "node",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dugajean/typescript-express"
  }
}
